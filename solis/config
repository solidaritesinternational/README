Host *
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    ServerAliveInterval 240

# Solis Servers
Host solis1
    HostName relay0.solis-demo.org
    User $name
    IdentityFile ~/.ssh/$name
    LocalForward 5000 localhost:5001
    LocalForward 5200 localhost:5201

