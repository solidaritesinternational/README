Host *
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    ServerAliveInterval 240

# Solis Bot
Host solisbot
    Hostname www.solis-bot.fr
    User ubuntu
    IdentityFile ~/.ssh/solis_staging.pem
    Port 22

Host solisbottest
    Hostname test.solis-bot.fr
    User ubuntu
    IdentityFile ~/.ssh/solis_staging.pem
    Port 22

Host solisbotdemo
    Hostname demo.solis-bot.fr
    User ubuntu
    IdentityFile ~/.ssh/solis_staging.pem
    Port 22



